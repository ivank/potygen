{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strict": true,
    "module": "commonjs",
    "target": "es2018",
    "sourceMap": false,
    "lib": ["es2018"],
    "outDir": "dist"
  }
}
