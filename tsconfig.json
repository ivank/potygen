{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strict": true,
    "module": "commonjs",
    "target": "ES2020",
    "sourceMap": true,
    "lib": ["ES2020"],
    "outDir": "dist"
  }
}
